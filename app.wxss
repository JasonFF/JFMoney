  url('//at.alicdn.com/t/font_959106_ovala7igxal.woff') format('woff'),
  url('//at.alicdn.com/t/font_959106_ovala7igxal.ttf') format('truetype'),
  url('//at.alicdn.com/t/font_959106_ovala7igxal.svg#iconfont') format('svg');
}
.iconfont{
    font-family:"iconfont" !important;
    font-size: 50rpx;font-style:normal;
    -webkit-font-smoothing: antialiased;
    -webkit-text-stroke-width: 0.2px;
    -moz-osx-font-smoothing: grayscale;
  }
.icon-pencil:before {
  content: "\e64c";
  color:cornflowerblue
}
.icon-loan:before {
  content: "\e61f";
  color:#FF6666
}
.icon-account:before {
  content: "\e60f";
  color: darkorange
}
.icon-property:before {
  content: "\e602";
  color:#99CC66
}

page {
  text-rendering: optimizeLegibility;
  -webkit-font-smoothing: antialiased;
    background: #f9f9f9;
